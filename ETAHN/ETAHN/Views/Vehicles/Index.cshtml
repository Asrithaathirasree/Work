@model IEnumerable<ETAHN.Vehicle>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Insurane_date_Renewal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RCBook_Details)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Service_DueDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OwnershipOfId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Having_LicenceId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OwnershipOf.Bike)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Having_Licence.Yes)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Insurane_date_Renewal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RCBook_Details)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Service_DueDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnershipOfId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Having_LicenceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnershipOf.Bike)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Having_Licence.Yes)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
