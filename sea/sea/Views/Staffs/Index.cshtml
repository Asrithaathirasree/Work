        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Staff_Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department)
