@model IEnumerable<sea.Admission_form>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p><a href="~/Admission_form/Index">Admission form</a>
        <a href="~/Students/Index">Student</a>
        <a href="~/Staffs/Index">Staff</a>
        <a href="~/Authorities/Index">Authorities</a>
        <a href="~/Classrooms/Index">Classroom</a>
        <a href="~/Maps/Index">Map</a>
        <a href="~/Libraries/Index">Library</a>
        <a href="~/Fees/Index">Fees</a>

</p>
    <p>
        Child_forms:
        <a href="~/Enquiry_Type/Index">Enquiry_Type</a>
        <a href="~/Student_Type/Index">Student_Type</a>
        <a href="~/Quotas/Index">Quota</a>

    </p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tenth_Percentage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plustwo_Percentage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone_Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Enquiry_Type.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student_Type.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quota.Name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Department)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tenth_Percentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plustwo_Percentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone_Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enquiry_Type.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student_Type.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quota.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
